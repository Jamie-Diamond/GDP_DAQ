        "y limit values": [min,max],    //Only required if y limit is "Set", the minimum and maximum values for the y limit
        
        //Actual data
        "data":                                 //The data provided as a list of lists.
        [
            [x-value, y-value, confidence],     //Each data point should have x and y values, the confidence is optional and will be implemented if I have time (in the style of varying size points).
            [x-value, y-value, confidence],
            [x-value, y-value, confidence],
            [x-value, y-value, confidence],
            [x-value, y-value, confidence],
            [x-value, y-value, confidence],
            [x-value, y-value, confidence],
            [x-value, y-value, confidence],
            [x-value, y-value, confidence],
        ]
    }
